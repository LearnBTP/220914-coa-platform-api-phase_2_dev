VIEW "V_COA_AUDIT"
AS SELECT T_COA_AUDIT.BUILDING,
T_COA_AUDIT.FLOOR ,
T_COA_AUDIT.CM_ID,
T_COA_AUDIT.SITE_ID,
T_COA_AUDIT.BUILDING_ID,
T_COA_AUDIT.FLOOR_ID,
T_COA_AUDIT.LAST_UPDATED_BY,
T_COA_AUDIT.SCAN_STATUS ,
CAST(T_COA_AUDIT.SCAN_PLANNED_DATE AS SECONDDATE) AS SCAN_PLANNED_DATE,
T_CM.CM as CM,
T_SITE.SITE as SITE
FROM T_COA_AUDIT as T_COA_AUDIT
INNER JOIN T_CM as T_CM
  on T_COA_AUDIT.CM_ID = T_CM.CM_ID
INNER JOIN T_SITE as T_SITE 
on T_COA_AUDIT.SITE_ID = T_SITE.SITE_ID
