VIEW "V_AQIDANDBOMJOIN" AS
select T_COA_AQID_MAIN.createdBy,T_COA_AQID_MAIN.createdBy_Name, T_COA_AQID_MAIN.createdBy_mail, T_COA_AQID_MAIN.modifiedBy, T_COA_AQID_MAIN.modifiedBy_Name, T_COA_AQID_MAIN.modifiedBy_mail, T_COA_AQID_MAIN.CM, T_COA_AQID_MAIN.Site, T_COA_AQID_MAIN.Aqid, T_COA_AQID_MAIN.Mfr, T_COA_AQID_MAIN.Program, T_COA_AQID_MAIN.Equipment_Name, T_COA_AQID_MAIN.Stack_item, T_COA_AQID_MAIN.GH_Site, T_COA_BOM_STRUCTURE.Group_Priority, T_COA_BOM_STRUCTURE.Equipment_Type, T_COA_AQID_MAIN.createdAt, T_COA_AQID_MAIN.modifiedAt, T_COA_BOM_STRUCTURE.Station from 
COM_APPLE_COA_T_COA_AQID_MAIN as T_COA_AQID_MAIN  inner join COM_APPLE_COA_T_COA_BOM_STRUCTURE as T_COA_BOM_STRUCTURE
on T_COA_AQID_MAIN.Aqid = T_COA_BOM_STRUCTURE.Aqid and T_COA_AQID_MAIN.Program = T_COA_BOM_STRUCTURE.Program and T_COA_AQID_MAIN.CM = T_COA_BOM_STRUCTURE.CM and T_COA_AQID_MAIN.Site = T_COA_BOM_STRUCTURE.Site
where (T_COA_BOM_STRUCTURE.Equipment_Type = 'Buy' or T_COA_BOM_STRUCTURE.Equipment_Type = 'Carry Over')
